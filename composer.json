{
    "name": "m1/stash-silex",
    "description": "StashSilex is a service provider and session handler for the popular caching library Stash",
    "keywords": [
        "m1",
        "stashsilex",
        "stash",
        "silex",
        "stash-silex",
        "service",
        "provider",
        "session",
        "handler",
        "serviceprovider",
        "sessionhandler",
        "extension",
        "apc",
        "cache",
        "caching",
        "memcached",
        "redis",
        "sessions"
    ],
    "homepage": "https://github.com/M1/StashSilex",
    "license": "MIT",
    "authors": [
        {
            "name": "Miles Croxford",
            "email": "hello@milescroxford.com",
            "homepage": "http://milescroxford.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "silex/silex": "1.*",
        "tedivm/stash": ">=0.11.1 < 1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "m1/vars": "~0.6",
        "monolog/monolog": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "M1\\StashSilex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M1\\StashSilex\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
